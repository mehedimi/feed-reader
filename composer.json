{
    "name": "mehedimi/feed-reader",
    "version": "1.0",
    "description": "A package for reading rss or atom feed",
    "license": ["MIT"],
    "authors": [
        {
            "name": "Mehedi Hasan",
            "email": "mehedihasansabbirmi@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-curl": "*",
        "ext-simplexml": "*",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Mehedi\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "symfony/var-dumper": "^5.0"
    },
    "extra": {
        "laravel": {
            "aliases": {
                "Feed": "Mehedi\\Facades\\Feed"
            }
        }
    }
}
